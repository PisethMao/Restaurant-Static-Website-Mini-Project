D:/ISTAD/Web Design/FINAL PROJECT/Restaurant-Static-Website-Mini-Project/#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,/,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/@tailwindcss/cli/dist/@tailwindcss/cli/dist/index.mjs" "$@"
fi
